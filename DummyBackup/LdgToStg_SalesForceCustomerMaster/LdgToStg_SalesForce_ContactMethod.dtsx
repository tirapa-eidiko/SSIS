              <properties>
                <property
                  dataType="System.Int32"
                  description="The number of seconds before a command times out.  A value of 0 indicates an infinite time-out."
                  name="CommandTimeout">0</property>
                <property
                  dataType="System.String"
                  description="Specifies the name of the database object used to open a rowset."
                  name="OpenRowset"></property>
                <property
                  dataType="System.String"
                  description="Specifies the variable that contains the name of the database object used to open a rowset."
                  name="OpenRowsetVariable"></property>
                <property
                  dataType="System.String"
                  description="The SQL command to be executed."
                  name="SqlCommand"
                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor, Microsoft.DataTransformationServices.Controls, Version=15.0.0.0, Culture=neutral, PublicKeyToken=89845dcd8080cc91">SELECT
      [Area_Code__c] AS AreaCode
     ,[LeadNumber__c] AS SourceCustomerId
     ,'SALESFORCE' AS SourceSystemName
	  ,CASE 
	   WHEN [MobilePhone] IS NOT NULL AND [Email] IS NULL THEN 'Telephone Number'
	   WHEN [MobilePhone] IS NULL AND [Email] IS NOT NULL THEN 'EMAIL'
	   WHEN [MobilePhone] IS NOT NULL THEN 'Telephone Number'
	   WHEN [Email] IS NOT NULL THEN 'Email Address'
	   ELSE null
	   END AS ContactMethodCategory
	   ,CASE 
	   WHEN [MobilePhone] IS NOT NULL AND [Email] IS NULL THEN 'Mobile Telephone'
	   WHEN [MobilePhone] IS NULL AND [Email] IS NOT NULL THEN 'Primary Email'
	   WHEN [MobilePhone] IS NOT NULL THEN 'Mobile Telephone'
	   WHEN [Email] IS NOT NULL THEN 'Primary Email'
	   ELSE null
	   END AS ContactMethodType
	   ,CASE
	   WHEN [MobilePhone] IS NOT NULL AND [Email] IS NULL THEN [MobilePhone]
	   WHEN [MobilePhone] IS NULL AND [Email] IS NOT NULL THEN [Email]
	   WHEN [MobilePhone] IS NOT NULL THEN [MobilePhone]
	   WHEN [Email] IS NOT NULL THEN [Email]
	   ELSE null
	   END AS ContactMethodValue
	  ,[Area_Code__c] AS AreaCode1
     ,[LeadNumber__c] AS SourceCustomerId1
     ,'SALESFORCE' AS SourceSystemName1
	  ,CASE 
	   WHEN [MobilePhone] IS NOT NULL AND [Email] IS NULL THEN 'Telephone Number'
	   WHEN [MobilePhone] IS NULL AND [Email] IS NOT NULL THEN 'EMAIL'
	   WHEN [MobilePhone] IS NOT NULL THEN 'Telephone Number'
	   WHEN [Email] IS NOT NULL THEN 'Email Address'
	   ELSE null
	   END AS ContactMethodCategory1
	   ,CASE 
	   WHEN [MobilePhone] IS NOT NULL AND [Email] IS NULL THEN 'Mobile Telephone'
	   WHEN [MobilePhone] IS NULL AND [Email] IS NOT NULL THEN 'Primary Email'
	   WHEN [MobilePhone] IS NOT NULL THEN 'Mobile Telephone'
	   WHEN [Email] IS NOT NULL THEN 'Primary Email'
	   ELSE null
	   END AS ContactMethodType1
	   ,CASE
	    WHEN [MobilePhone] IS NOT NULL AND [Email] IS NOT NULL THEN 'BOTH' 
		WHEN [MobilePhone] IS NOT NULL THEN 'Phone'
		WHEN [MobilePhone] IS NOT NULL THEN 'Email'
		ELSE 'None'
		END AS HasBothModes
	 ,[MobilePhone] As MobilePhone
	 ,[Email] AS Email 
	 , 'Email Address' AS EmailCategory 
	 , 'Telephone Number' AS  PhoneCategory
	 , 'Primary Email' AS EmailMethodType
	 , 'Mobile Telephone' AS PhoneMethodType
	 ,'BOTH' As Both
  FROM [Landing].[dbo].[LDG_SALESFORCE_USERS]</property>
