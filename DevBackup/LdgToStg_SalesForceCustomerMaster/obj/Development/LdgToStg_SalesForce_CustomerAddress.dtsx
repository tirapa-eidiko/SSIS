              <properties>
                <property
                  dataType="System.Int32"
                  description="The number of seconds before a command times out.  A value of 0 indicates an infinite time-out."
                  name="CommandTimeout">0</property>
                <property
                  dataType="System.String"
                  description="Specifies the name of the database object used to open a rowset."
                  name="OpenRowset"></property>
                <property
                  dataType="System.String"
                  description="Specifies the variable that contains the name of the database object used to open a rowset."
                  name="OpenRowsetVariable"></property>
                <property
                  dataType="System.String"
                  description="The SQL command to be executed."
                  name="SqlCommand"
                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor, Microsoft.DataTransformationServices.Controls, Version=15.0.0.0, Culture=neutral, PublicKeyToken=89845dcd8080cc91">SELECT
     [LeadNumber__c] AS SourceCustomerId
	 ,'SALESFORCE' AS SourceSystemName
	 ,'Primary Address' AS ADDRESS_TYPE
	 ,LTRIM(RTRIM(STREET)) +', '+LTRIM(RTRIM(CITY)) +', '+LTRIM(RTRIM(COUNTRY))+', '+LTRIM(RTRIM(PostalCode))
	 AS ADDRESS_LINE_ONE
	 ,LTRIM(RTRIM(PostalCode))AS ZIPCODE
	 ,LTRIM(RTRIM(CITY)) AS CITY
	 ,LTRIM(RTRIM(STATE)) AS STATE
	 ,LTRIM(RTRIM(COUNTRY)) AS COUNTRY
	 ,CASE
      WHEN [HasOptedOutOfEmail] = 'True'  AND [HasOptedOutOfFax] = 'True' THEN '1'
      WHEN [HasOptedOutOfEmail] = 'True' THEN '1'
	  WHEN [HasOptedOutOfFax] = 'True' THEN '1'
      ELSE null
	  END AS OptionOptOutFlag
	 FROM [LANDING].[dbo].[LDG_SALESFORCE_USERS]
	 WHERE ISNULL(LTRIM(RTRIM(STREET)),'') &lt;&gt; '' AND ISNULL(LTRIM(RTRIM(CITY)),'')&lt;&gt;''</property>
                <property
