                <property
                  dataType="System.String"
                  description="The SQL command to be executed."
                  name="SqlCommand"
                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor, Microsoft.DataTransformationServices.Controls, Version=15.0.0.0, Culture=neutral, PublicKeyToken=89845dcd8080cc91">SELECT 
	ID
	,SourceCustomerId
	,SourceSystemName
	,PersonOrgType
	,LTRIM(RTRIM(FirstName)) as FirstName
	,LTRIM(RTRIM(MiddleName)) as MiddleName
	,Case 
			When IsNULL(LTRIM(RTRIM(LastName)),'') = '' AND UPPER(LTRIM(RTRIM(PersonOrgType)))='P' THEN 'UNKNOWN' Else LTRIM(RTRIM(LastName))
	End as LastName
	,AliasName
	,Salutation
	,LTRIM(RTRIM(OrganizationName)) as OrganizationName
	,LTRIM(RTRIM(DisplayName)) as DisplayName
	,Gender
	,DateOfBirth
	,PlaceOfBirth
	,Case	    
		 When UPPER(LTRIM(RTRIM(IsNULL(MaritalStatus,'')))) in ('M','1','MARRIED') then 'Married'
		 When UPPER(LTRIM(RTRIM(IsNULL(MaritalStatus,'')))) in ('U','0','UNMARRIED') then 'Unmarried'
		 Else NULL
	 end as MaritalStatus
	,CreateDate
	,ModifiedDate
	,IsActive
	,ProfileUrl
	,AgeGroup
	,Nationality
	,Case
	     When UPPER(PersonOrgType)='O' and UPPER(SourceSystemName) in ('YARDI','VGS','ORACLE PROPERTY') then 'UNDEFINED'
		 Else OrgType
	End as OrgType
	,VipFlag
	,PrefCommunicationChannel
	,ContactSource
	,Case
	      When LTRIM(RTRIM(IsNULL(PreferredLanguage,''))) = '' then 'English'
		  Else  LTRIM(RTRIM(PreferredLanguage))
	End as PrefferedLanguage
	,DocumentStatus
	,ContactType
	,CustomerClass
	,DataSource
	,ArabicName
	,RecordTimeStamp
	,IsProspect
	,IsValid
	,ThirdName
	,NickName
	,ResidentialStatus
	,PreferredCommunicationFrequency
	,MDM_DateOfBirth
	,MDM_Gender
	,MDM_Salutation
	,LeaseName
	,PropertyCode
	,PropertyName
	,TenantStatus
